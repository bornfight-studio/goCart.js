{
  "name": "@bornfight/mario-cart",
  "version": "1.0.0",
  "description": "Mario Cart - Shopify Ajax cart library",
  "keywords": [
    "shopify",
    "ajax",
    "cart",
    "drawer"
  ],
  "homepage": "https://github.com/bornfight/goCart.js",
  "bugs": {
    "url": "https://github.com/bornfight/goCart.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bornfight/goCart.js"
  },
  "license": "MIT",
  "author": "Bornfight Studio <hello@bornfight.studio> (https://github.com/bornfight)",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "webpack && tsc",
    "coverage": "npm run test -- --coverage",
    "format": "prettier --write 'src/**/*.{scss,js,ts}'",
    "prepare": "husky install",
    "test": "jest --silent",
    "trypublish": "npm publish || true",
    "watch": "webpack --watch && tsc"
  },
  "browserslist": [
    "defaults"
  ],
  "jest": {
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less|scss)$": "<rootDir>/scripts/testMock.js"
    }
  },
  "dependencies": {
    "axios": "^1.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-typescript": "^7.21.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "babel-preset-minify": "^0.5.2",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "engines": {
    "node": ">=14.20.1",
    "npm": ">=8.19.2"
  },
  "email": "hello@bornfight.studio"
}
